/**
 * 16-bit bitwise Or:
 * for i = 0..15 out[i] = (a[i] or b[i])
 */

CHIP Or16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
    Nand(a=a,b=a,out=a1nandb1);
    Nand(a=b,b=b,out=a2nandb2);
    Nand(a=a1nandb1,b=a2nandb2,out=out);
}
