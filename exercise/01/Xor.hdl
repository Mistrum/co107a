/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=b,b=b,out=ynandy);
    Nand(a=a,b=ynandy,out=xnandyy);
    Nand(a=a,b=a,out=xnandx);
    Nand(a=xnandx,b=b,out=xxnandy);
    Nand(a=xnandyy,b=xxnandy,out=out);
}
